

         //check whether or not the file appears in the deleted files table
         var authKey = request.parameters['authKey'];
         var scriptName = request.parameters['scriptName'];
         var sql = "SELECT count(*) result FROM GIT_FILES_DELETED WHERE name='"+ scriptName +"' and authKey='" + authKey +"'";
      
         var response = testHelper.runQuery(sql);
         console.log(response);
         if(response[0].result == "1"){
	    	 return {"result":"success"};
	     }else{
	    	return {"result":"failure"};
	     }
